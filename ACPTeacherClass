class Teacher
{
  Course[8] schedule;
  int teacherID;
  
  public:
  void Teacher();
  void ~Teacher();
  void Teacher(Course[] courses, int id);
}

void Teacher::Teacher(){
  for(int count = 0; count < 8; count++)
    schedule[count] = NULL;
  teacherID = 0;
}

void Teacher::~Teacher(){
  for(int count = 0; count < 8; count++)
    free(schedule[count]);
  free(schedule);
  free(teacherID);
}

void Teacher::Teacher(Course[] courses, int id){
  for(int count = 0; count < 8; count++)
    schedule[count] = couses[count];
  teacherID = id;
}
